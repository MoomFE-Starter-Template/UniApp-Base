{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>moomfe/renovate-config"
  ],
  "ignoreDeps": [
    // UniApp 支持的 Vue 版本
    "vue",
    "pinia",

    // 使用了 pnpm 打补丁的包
    "vue-demi",
    "@vueuse/*",

    // https://github.com/Moonofweisheng/wot-design-uni/issues/716
    "sass",

    // 暂缓升级这几个依赖, 会导致小程序端启动失败
    "vite",
    "unocss",
    "unocss-applet",
    "@unocss-applet/*"
  ]
}
